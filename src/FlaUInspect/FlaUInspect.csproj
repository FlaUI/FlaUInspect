    </ItemGroup>

    <ItemGroup>
        <None Update="FlaUInspect.png">
            <Pack>True</Pack>
            <PackagePath></PackagePath>
        </None>
    </ItemGroup>

    <ItemGroup>
        <None Remove="LICENSE" />
        <None Remove="Resources\ListView.xaml~" />
    </ItemGroup>

</Project>
